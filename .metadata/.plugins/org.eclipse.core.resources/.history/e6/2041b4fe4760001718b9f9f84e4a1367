//Tangy F.
//Displaying odd numbers less than 40 using inheritance

package fortylesspack;

public class Fortylessmain {

	public static void main(String[] args) {

	}

	

	public int odd(int w ){
		//declare a variable 
		int r=1;

		do {
			 System.out.println(r);

		 r=r+2;
	} 
	while ( r<=39);
		
		return r;
	
	}
		
	
	}		
	
